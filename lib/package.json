{
    "name": "dops",
    "version": "1.0.0",
    "description": "base boilerplate for construct npm pkgs",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "jest --config jestconfig.json",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "bin": {
        "dops": "./lib/src/index.js"
    },
    "preferGlobal": true,
    "keywords": [],
    "author": "Ignacio Barreto",
    "license": "MIT",
    "devDependencies": {
        "@types/aws-sdk": "^2.7.0",
        "@types/cli-table": "^0.3.0",
        "@types/handlebars": "^4.1.0",
        "@types/jest": "^24.0.18",
        "@types/js-yaml": "^3.12.1",
        "@types/lodash": "^4.14.138",
        "@types/node": "^12.7.2",
        "@types/ora": "^3.2.0",
        "@types/vorpal": "^1.11.0",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.19.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.6.2"
    },
    "dependencies": {
        "aws-sdk": "^2.520.0",
        "cli-table": "^0.3.1",
        "handlebars": "^4.1.2",
        "js-yaml": "^3.13.1",
        "lodash": "^4.17.15",
        "ora": "^3.4.0",
        "vorpal": "^1.12.0"
    }
}
